    "express": "^4.21.0",
    "mongodb": "^6.9.0",
    "mongoose": "^8.6.2",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.4"
  }
}
